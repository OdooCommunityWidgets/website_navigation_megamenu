<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <template id="custom_navigation_menuitem_extended" inherit_id="website.menu_tree" name="website_custom_navigation_menuitem_extended" customize_show="true" priority="1">
            <xpath expr="//tree[@string='Website menu']" position="replace">
             <tree string="Website menu" editable="bottom">
                    <field name="name"/>
                    <field name="url"/>
                    <field name="new_window"/>
                    <field name="parent_id"/>
                    <field name="sequence"/>
                </tree>
            </xpath>
        </template>
    </data>
</openerp>
